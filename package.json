{
  "name": "assui",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com:SPOTEC-LTD/assui.git"
  },
  "scripts": {
    "init": "rm -rf node_modules && yarn install && yarn run clean && yarn run bootstrap && yarn run build",
    "start": "yarn run dev",
    "dev": "dumi dev",
    "bootstrap": "lerna bootstrap",
    "clean": "lerna clean --yes",
    "build": "lerna run build",
    "lerna": "lerna",
    "test": "jest --watchAll",
    "lint": "eslint --ignore-pattern **/__tests__/* --ignore-pattern **/demo/* \"packages/*/src/**/*.{ts,tsx}\"",
    "build:doc": "dumi build",
    "pub:doc-gitee": "npm run build:doc && cd ./dist && rm -rf .git && touch .spa && touch .nojekyll && git init && git remote add origin git@gitee.com:uso/assui.git && git add -A && git commit -m \"publish docs\" && git push origin master -f",
    "pub": "lerna publish"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@svgr/webpack": "^5.5.0",
    "@testing-library/react": "^10.0.4",
    "@testing-library/react-hooks": "^2.0.1",
    "@types/jest": "^24.9.1",
    "@types/lodash.debounce": "^4.0.6",
    "@types/lodash.isequal": "^4.5.5",
    "@types/lodash.throttle": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "@umijs/fabric": "^2.1.0",
    "@umijs/plugin-sass": "^1.1.1",
    "antd": "^4.16.11",
    "babel-loader": "^8.1.0",
    "babel-plugin-import": "^1.12.0",
    "babel-plugin-transform-async-to-promises": "^0.8.15",
    "del": "^5.1.0",
    "dumi": "^1.1.26",
    "enzyme": "^3.10.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-compat": "^3.12.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-markdown": "^2.2.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-unicorn": "^35.0.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "jest-fetch-mock": "^3.0.3",
    "lerna": "^2.11.0",
    "less": "^4.1.1",
    "less-plugin-npm-import": "^2.1.0",
    "mockdate": "^3.0.2",
    "mockjs": "^1.1.0",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "react": "^17.0.2",
    "react-color": "^2.19.3",
    "react-copy-to-clipboard": "^5.0.3",
    "react-dom": "^17.0.2",
    "react-drag-listview": "^0.1.6",
    "react-test-renderer": "^16.13.1",
    "surge": "^0.21.3",
    "ts-jest": "^26.1.0",
    "typescript": "^3.3.3",
    "umi-request": "^1.2.18",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.10",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "insert-css": "^2.0.0",
    "rc-motion": "^2.4.4"
  }
}
