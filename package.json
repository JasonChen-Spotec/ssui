{
  "name": "assui",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com:SPOTEC-LTD/assui.git"
  },
  "scripts": {
    "prepare": "husky install",
    "init": "rm -rf node_modules && yarn install && yarn run clean && yarn run bootstrap && yarn run build",
    "start": "yarn run dev",
    "dev": "dumi dev",
    "bootstrap": "lerna bootstrap",
    "clean": "lerna clean --yes",
    "build": "lerna run build",
    "lerna": "lerna",
    "test": "jest --watchAll",
    "eslint": "eslint --quiet --cache --ext .js,.jsx,.ts,.tsx ./packages",
    "eslint:fix": "eslint --quiet --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./packages",
    "stylelint": "stylelint 'packages/**/*.less'",
    "stylelint:fix": "stylelint --fix \"packages/**/*.less\" --syntax less",
    "prettier": "prettier --write 'packages/**/*.{js,jsx,tsx,ts,less,md,json}'",
    "build:doc": "dumi build",
    "pub:doc-gitee": "npm run build:doc && cd ./dist && rm -rf .git && touch .spa && touch .nojekyll && git init && git remote add origin git@gitee.com:uso/sui.git && git add -A && git commit -m \"publish docs\" && git push origin master -f",
    "pub": "lerna publish"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@svgr/webpack": "^5.5.0",
    "@testing-library/react": "^10.0.4",
    "@testing-library/react-hooks": "^2.0.1",
    "@types/jest": "^24.9.1",
    "@types/lodash.debounce": "^4.0.6",
    "@types/lodash.isequal": "^4.5.5",
    "@types/lodash.throttle": "^4.1.6",
    "@umijs/plugin-sass": "^1.1.1",
    "antd": "^4.16.11",
    "babel-loader": "^8.1.0",
    "babel-plugin-import": "^1.12.0",
    "babel-plugin-transform-async-to-promises": "^0.8.15",
    "code-rule": "^1.1.5",
    "del": "^5.1.0",
    "dumi": "^1.1.26",
    "enzyme": "^3.10.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "husky": "^7.0.2",
    "jest": "^26.0.1",
    "jest-fetch-mock": "^3.0.3",
    "lerna": "^4.0.0",
    "less": "^4.1.1",
    "less-plugin-npm-import": "^2.1.0",
    "mockdate": "^3.0.2",
    "mockjs": "^1.1.0",
    "pretty-quick": "^2.0.1",
    "react": "^17.0.2",
    "react-color": "^2.19.3",
    "react-copy-to-clipboard": "^5.0.3",
    "react-dom": "^17.0.2",
    "react-drag-listview": "^0.1.6",
    "react-test-renderer": "^16.13.1",
    "surge": "^0.21.3",
    "ts-jest": "^26.1.0",
    "typescript": "^3.3.3",
    "umi-request": "^1.2.18",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.10",
    "webpack-merge": "^4.2.2"
  }
}
